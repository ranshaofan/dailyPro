.flexCenter{
  display: flex;
  justify-content: center;
  align-items: center;
}
.purple{
  color: #15399C;
}
.page{
  width: 100vw;
  height: 100vh;
  padding: 0 30px;
  box-sizing: border-box;
  overflow: scroll;
}
/* 日历开始 */
.calendarView{
  width: 100%;
  border: 1px solid #D1CECE;
  border-radius: 20px;
  white-space: nowrap;
  padding: 12px 12px 8px 12px;
  box-sizing: border-box;
  margin: 30px 0;
  box-shadow: 0 0 99px rgba(231,205,241,0.47);
}
.calenList{
    display: inline-flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    width: 35px;
    padding: 0 0 4px 0;
    border-radius: 14px;
  }
  .chosen{
  background-color: #7857D2 !important;
  color: #fff;
}
.pointY{
  display: inline-block;
  background-color: #ffec1f;
  width: 8px;
  height: 8px;
  border-radius: 20px;
  box-shadow: 0 0 10px #8d8b8b;
}
.calenToday{
  background-color: #e4dff1;
  border-radius: 10px ;

}
/* cost */
.costBox{
  position: relative;
  height: 88px;
  border-radius: 16px;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  box-shadow: 0 3px 6px rgba(0, 0, 0, 0.16);
  margin-bottom: 20px;
  overflow: hidden;
}
.costBoxin{
  position: absolute;
  left: 0;
  top: 0;
  border-radius: 16px;
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding: 0 16px;
  box-sizing: border-box;
  z-index: 2;
}
.costImgView{
  width: 40px;
  height: 40px;
  border-radius: 40px;
  border: 1px solid #70707042;
  background-color: #fff;
  margin-right: 20x;
}
.costTxt{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: flex-start;
  margin-left: 16px;
}
.costtype{
  color: #3D1467;
  font-size: 16px;
  font-weight: 700;
  margin-bottom: 3px;
}
.costcon{
  font-size: 13px;
  color: #655B6F;
}
.moreImgView{
  position: absolute;
  right: 18px;
  top: 0;
  bottom: 0;
  margin: auto;
  font-size: 14px;
}
.costImgView{
  width: 40px;
  height: 40px;
  border-radius: 40px;
  border: 1px solid #70707042;
  background-color: #fff;
  margin-right: 20x;
}
.costImgView image{
  width: 28px;
  height: 28px;
}
.block{
  font-size: 20px;
  color: #34353a;
  width: 100%;
  height: 60px;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding-left: 40px;
  border-top: 1px solid #ccc;
  border-bottom: 1px solid #ccc;
  margin: 10px 0;
  background-color: #fff;
  box-shadow: 0 0 10px #ccc;
}
/*加载loading**/
.container {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 100vh;
  background-color: #f8f8f8;
}

.loading-spinner {
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: rgba(198, 186, 198, 0.6);
  width: 100vw;
  height: 100vh;
  flex-direction: column;
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 20240327;
}
/* .loading-spinner view{
  color: #481e62;
  font-size: 18px;
} */

.container {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 100vh;
  background-color: #f8f8f8;
}

.spinner-image {
  width: 50px;
  height: 50px;
  animation: spin 1s linear infinite;
}

@keyframes spin {
  0% {
    transform: rotate(360deg);
  }
  100% {
    transform: rotate(0deg);
  }
}

.textspan{
  color: #9c9a9d;
  font-size: 18px;
  font-family:Cambria, Cochin, Georgia, Times, 'Times New Roman', serif;
  background-color: #fff;
  border: 1px solid #E7E7EB;
  padding: 10px;
  border-radius: 20px;
  box-shadow: 0 0px 10px #ccc;
  margin: 10px;
}
.typenoneSpan{
  color: #9c9a9d;
  font-size: 14px;
  font-family:Cambria, Cochin, Georgia, Times, 'Times New Roman', serif;
  margin: 5px;
  text-align: center;
}
.editclass{
